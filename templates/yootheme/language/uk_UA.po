msgid "You must be <a href=\"%s\">logged in</a> to post a comment."
msgstr "Ви повинні <a href=\"%s\"> увійти </a> щоб залишити коментар."

#, php-format
msgid "Logged in as <a href=\"%1$s\">%2$s</a>. <a href=\"%3$s\" title=\"Log out of this account\">Log out?</a>"
msgstr "Ви зайшли як <a href=\"%1$s\">%2$s</a>. <a href=\"%3$s\" title=\"Log out of this account\">Вийти?</a>"

